{
  "name": "dapper-apps.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "prebuild:css": "postcss --use autoprefixer ./views/sass/*.scss -d ./views/sass/",
    "build:css": "node-sass --output-style compressed ./views/sass/ -o ./public/styles/",
    "prebuild:js": "jshint ./public/scripts/*",
    "build:js": "",
    "build": "npm run build:css && npm run build:js",
    "env": "env"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "jshint": "^2.9.1",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.19",
    "postcss-cli": "^2.5.1",
    "postcss-scss": "^0.1.7"
  }
}
